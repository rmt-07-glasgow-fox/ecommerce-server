{
  "name": "ecommerce-server",
  "version": "1.0.0",
  "description": "Ecommerce application",
  "main": "app.js",
  "scripts": {
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create:development": "NODE_ENV=development npm run db:create",
    "db:create:test": "NODE_ENV=test npm run db:create",
    "db:drop:development": "NODE_ENV=development npm run db:drop",
    "db:drop:test": "NODE_ENV=test npm run db:drop",
    "db:migrate:development": "NODE_ENV=development npm run db:migrate",
    "db:migrate:test": "NODE_ENV=test npm run db:migrate",
    "db:seed:development": "NODE_ENV=development npm run db:seed",
    "db:seed:test": "NODE_ENV=test npm run db:seed",
    "db:seed:undo:development": "NODE_ENV=development npm run db:seed:undo",
    "db:seed:undo:test": "NODE_ENV=test npm run db:seed:undo",
    "test": "NODE_ENV=test npx jest",
    "dev": "NODE_ENV=development npx nodemon bin/http.js",
    "start": "node bin/http.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helmiz/ecommerce-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/helmiz/ecommerce-server/issues"
  },
  "homepage": "https://github.com/helmiz/ecommerce-server#readme",
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5"
  }
}
