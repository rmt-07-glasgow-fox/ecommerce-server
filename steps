"db:create:development":"NODE_ENV=development npx sequelize-cli db:create",
"db:create:test":"NODE_ENV=test npx sequelize-cli db:create",
"db:migrate:development":"NODE_ENV=development npx sequelize-cli db:migrate",
"db:migrate:test":"NODE_ENV=test npx sequelize-cli db:migrate",
"test": "NODE_ENV=test npx jest"

NODE_ENV=test npx sequelize db:migrate:undo:all

npm install --save-dev jest supertest

